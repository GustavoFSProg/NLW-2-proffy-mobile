{
  "expo": {
    "name": "Proffy",
    "slug": "mobile",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/icon.png",
    "splash": {
      "image": "src/assets/splash.png",

      "resizeMode": "contain",
      "backgroundColor": "#7d40E7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "package": "com.company.proffyApp"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    }
  }
}
